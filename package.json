{
  "name": "grant-ai",
  "version": "1.0.0",
  "description": "Multilingual, agentic proposal generation engine with chaos-resilient orchestration",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "vite frontend --port 3001",
    "build": "vite build frontend",
    "start": "node backend/server.js",
    "migrate": "prisma migrate deploy",
    "migrate:test": "DATABASE_URL=$TEST_DATABASE_URL prisma migrate deploy",
    "create-admin": "node scripts/create-admin.js",
    "setup:dev": "npm run migrate && node scripts/create-test-data.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --dir tests/unit --reporter verbose",
    "test:integration": "vitest run --dir tests/integration --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:ci": "vitest run --coverage && playwright test",
    "lint": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "prepare": "husky install",
    "chaos:test": "vitest run --dir tests/chaos",
    "chaos:report": "node scripts/generate-chaos-report.js",
    "docker:build": "docker-compose -f docker/docker-compose.yml build --no-cache",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d --force-recreate",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "deploy": "npm run build && npm run docker:build && npm run docker:up",
    "cleanup:test": "node scripts/cleanup-test-data.js",
    "docs": "typedoc --out docs backend/ frontend/",
    "health-check": "node scripts/health-check.js",
    "postinstall": "husky install"
  },
  "keywords": [
    "grant-writing",
    "ai",
    "proposal-generation",
    "chaos-engineering",
    "multilingual",
    "enterprise",
    "resilience",
    "typescript"
  ],
  "author": "Grant-AI Team <engineering@grant-ai.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@google-cloud/text-to-speech": "^5.8.1",
    "@google/generative-ai": "^0.15.0",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-charts": "^6.19.1",
    "@sentry/node": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "@sentry/react": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.5.0",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "chromadb": "^3.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "html-to-text": "^9.0.5",
    "i18n": "^0.15.3",
    "i18next": "^23.11.5",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.2.0",
    "logrocket": "^10.1.0",
    "logrocket-react": "^6.0.3",
    "mongoose": "^8.3.4",
    "nodemailer": "^6.9.13",
    "nodemailer-dkim": "^1.0.0",
    "ollama": "^0.5.0",
    "openai": "^4.47.0",
    "puppeteer": "^22.9.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "recharts": "^3.3.0",
    "redis": "^5.9.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "vite": "^5.2.11",
    "wavesurfer.js": "^7.8.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/nodemailer": "^6.4.14",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "happy-dom": "^20.0.10",
    "husky": "^9.0.11",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.11.6",
    "nodemon": "^3.1.0",
    "playwright": "^1.56.1",
    "prettier": "^3.2.5",
    "prisma": "^5.13.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "prisma": ">=5.0.0"
  },
  "prisma": {
    "schema": "backend/prisma/schema.prisma",
    "seed": "node backend/prisma/seed.js"
  },
  "nodemonConfig": {
    "watch": [
      "backend/",
      "frontend/",
      "tests/"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "dist/",
      "coverage/",
      "test-results/"
    ],
    "exec": "node backend/server.js"
  }
}
